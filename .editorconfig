# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf #standard for windows and linux compatiblity
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = true #prevents the need to enter a new line on the end of files
